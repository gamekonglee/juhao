<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
    android:id="@+id/main_sv">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="140dp"
            android:background="@drawable/icon_top">
            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/head_user_cv"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:src="@drawable/touxian"
                android:layout_marginLeft="20dp"
                android:layout_centerVertical="true"
                />
            <TextView
                android:id="@+id/nickname_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="昵称"
                android:layout_marginLeft="15dp"
                android:textColor="@color/white"
                android:layout_toRightOf="@id/head_user_cv"
                android:textSize="17dp"
                android:layout_marginTop="55dp"
                />
            <TextView
                android:id="@+id/level_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="级别"
                android:layout_marginLeft="15dp"
                android:textColor="#e6e6e6"
                android:layout_toRightOf="@id/head_user_cv"
                android:textSize="15dp"
                android:layout_marginTop="80dp"
                />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/order_rl"
                style="@style/mine_item_style02"
                >


                <TextView
                    android:id="@+id/ordert_tv"
                    style="@style/mine_text_style"
                    android:text="@string/my_order"/>

                <ImageView
                    android:id="@+id/arror02_iv"
                    style="@style/mine_image_arrow"
                    android:layout_alignParentRight="true"/>
                <TextView
                    style="@style/mine_text_style"
                    android:layout_toLeftOf="@id/arror02_iv"
                    android:layout_marginRight="5dp"
                    android:textSize="12dip"
                    android:text="@string/look_all_order"/>

            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@color/white"
                android:paddingBottom="10dp"
                android:paddingTop="10dp">
                <RelativeLayout
                    android:id="@+id/payment_rl"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/payment_iv"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/payment" />

                    <TextView
                        android:layout_marginTop="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/payment_iv"
                        android:layout_centerHorizontal="true"
                        android:text="@string/payment" />
                    <TextView
                        android:id="@+id/unMessageReadTv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bg_unread_count"
                        android:gravity="center"
                        android:textColor="#FFFFFF"
                        android:visibility="gone"
                        android:textSize="12sp"
                        android:layout_alignRight="@id/payment_iv"
                        />

                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/delivery_rl"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/delivery_iv"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/delivery" />

                    <TextView
                        android:layout_marginTop="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/delivery_iv"
                        android:layout_centerHorizontal="true"

                        android:text="@string/delivery" />
                    <TextView
                        android:id="@+id/unMessageRead02Tv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/bg_unread_count"
                        android:gravity="center"
                        android:textColor="#FFFFFF"
                        android:visibility="gone"
                        android:textSize="12sp"
                        android:layout_alignRight="@id/delivery_iv"
                        />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/Receiving_rl"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/Receiving_iv"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@drawable/receiving" />

                    <TextView
                        android:layout_marginTop="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/Receiving_iv"
                        android:layout_centerHorizontal="true"
                        android:text="@string/Receiving" />
                    <TextView
