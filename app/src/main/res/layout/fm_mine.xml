                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="5dp"
                            android:textColor="@color/fontColor2"
                            android:text="设置"/>
                    </LinearLayout>
                    <LinearLayout
