<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/main_sv">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:background="@mipmap/bg_personal">
            <LinearLayout
                android:id="@+id/message_ll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="25dp"
                android:layout_marginRight="15dp"
                android:orientation="vertical"
                android:layout_alignParentRight="true"
                android:gravity="center">
                <ImageView
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:src="@mipmap/personal_news"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/setting_ll"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="25dp"
                android:layout_marginLeft="15dp"
                android:gravity="center">
                <ImageView
                    android:layout_width="25dp"
                    android:layout_height="25dp"
                    android:src="@mipmap/personal_setup"
                    />
            </LinearLayout>

            <de.hdodenhof.circleimageview.CircleImageView
                android:id="@+id/head_user_cv"
                android:layout_width="90dp"
                android:layout_height="90dp"
                android:src="@drawable/touxian"
                android:layout_marginLeft="20dp"
                android:layout_marginTop="80dp"
                />
            <TextView
                android:id="@+id/nickname_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="昵称"
                android:layout_marginLeft="15dp"
                android:textColor="#444444"
                android:layout_toRightOf="@id/head_user_cv"
                android:textSize="15dp"
                android:layout_marginTop="100dp"
                />
            <TextView
                android:id="@+id/level_tv"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="级别"
                android:layout_marginLeft="15dp"
                android:textColor="#444444"
                android:layout_toRightOf="@id/head_user_cv"
                android:textSize="12dp"
                android:layout_marginTop="120dp"
                />

        </RelativeLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <RelativeLayout
                android:id="@+id/order_rl"
                style="@style/mine_item_style02"
                >


                <TextView
                    android:id="@+id/ordert_tv"
                    style="@style/mine_text_style"
                    android:text="@string/my_order"/>

                <ImageView
                    android:id="@+id/arror02_iv"
                    style="@style/mine_image_arrow"
                    android:layout_alignParentRight="true"/>
                <TextView
                    style="@style/mine_text_style"
                    android:layout_toLeftOf="@id/arror02_iv"
                    android:layout_marginRight="5dp"
                    android:textSize="12dip"
                    />



            </RelativeLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="@color/white"
                android:paddingBottom="10dp"
                android:paddingTop="10dp">
                <RelativeLayout
                    android:id="@+id/payment_rl"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/payment_iv"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@mipmap/personal_pending_payment" />

                    <TextView
                        android:layout_marginTop="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/payment_iv"
                        android:layout_centerHorizontal="true"
                        android:textSize="12sp"
                        android:textColor="#444444"
                        android:text="@string/payment" />
                    <TextView
                        android:id="@+id/unMessageReadTv"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:background="@mipmap/personal_number"
                        android:gravity="center"
                        android:textColor="#FFFFFF"
                        android:layout_alignRight="@id/payment_iv"
                        android:textSize="12sp"
                        android:visibility="invisible" />
                </RelativeLayout>


                <RelativeLayout
                    android:id="@+id/delivery_rl"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/delivery_iv"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@mipmap/personal_pending_deliver" />

                    <TextView
                        android:layout_marginTop="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/delivery_iv"
                        android:layout_centerHorizontal="true"
                        android:textSize="12sp"
                        android:textColor="#444444"
                        android:text="@string/delivery" />
                    <TextView
                        android:id="@+id/unMessageRead02Tv"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:background="@mipmap/personal_number"
                        android:gravity="center"
                        android:textColor="#FFFFFF"
                        android:visibility="invisible"
                        android:textSize="12sp"
                        android:layout_alignRight="@id/delivery_iv"
                        />

                </RelativeLayout>
                <RelativeLayout
                    android:id="@+id/Receiving_rl"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:orientation="vertical">

                    <ImageView
                        android:id="@+id/Receiving_iv"
                        android:layout_width="35dp"
                        android:layout_height="35dp"
                        android:layout_centerHorizontal="true"
                        android:src="@mipmap/personal_pending_recipient" />

                    <TextView
                        android:layout_marginTop="5dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/Receiving_iv"
                        android:layout_centerHorizontal="true"
                        android:textSize="12sp"
                        android:textColor="#444444"
                        android:text="@string/Receiving" />
                    <TextView
                        android:id="@+id/unMessageRead03Tv"
                        android:layout_width="18dp"
                        android:layout_height="18dp"
                        android:background="@mipmap/personal_number"
                        android:gravity="center"
                        android:textColor="#FFFFFF"
                        android:visibility="invisible"
                        android:textSize="12sp"
                        android:layout_alignRight="@id/Receiving_iv"
                        />

                </RelativeLayout>

            </LinearLayout>

            <LinearLayout
                android:layout_marginTop="10dp"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:orientation="vertical"
